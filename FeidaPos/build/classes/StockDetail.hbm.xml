<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="org.hibernate.auction">

	<class name="com.orion.StockDetail" table="StockDetail">
		<id name="sdid" type="java.lang.String"></id>
		<property name="sid" not-null="true" length="20" column="`sid`"/>
		<property name="gid" not-null="true" length="20" column="`gid`"/>
		<property name="sdamount" not-null="true" column="`sdamount`"/>
		<property name="sdprice" not-null="true" column="`sdprice`"/>
		<property name="sdtotalprice" not-null="true" column="`sdtotalprice`"/>
	</class>
</hibernate-mapping>