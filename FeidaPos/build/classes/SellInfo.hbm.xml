<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="org.hibernate.auction">

	<class name="com.orion.SellInfo" table="SellInfo">
		<id name="eid" type="java.lang.String"></id>
		<property name="cid" not-null="true" length="20" column="`cid`"/>	
		<property name="edate" not-null="true" column="`edate`"/>
		<property name="etotalprice" not-null="true" column="`etotalprice`"/>
		<property name="eseller" not-null="true" length="50" column="`eseller`"/>	
	</class>
</hibernate-mapping>