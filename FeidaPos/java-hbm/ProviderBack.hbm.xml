<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="org.hibernate.auction">

	<class name="com.orion.ProviderBack" table="ProviderBack">
		<id name="pbid" type="java.lang.String"></id>
		<property name="pid" not-null="true" length="20" column="`pid`"/>
		<property name="sid" not-null="true" length="20" column="`sid`"/>	
		<property name="pbdate" not-null="true" column="`pbdate`"/>
	</class>
</hibernate-mapping>