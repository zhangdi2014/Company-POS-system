<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="org.hibernate.auction">

	<class name="com.orion.SellDetail" table="SellDetail">
		<id name="edid" type="java.lang.String"></id>
		<property name="eid" not-null="true" length="20" column="`eid`"/>
		<property name="gid" not-null="true" length="20" column="`gid`"/>
		<property name="edamount" not-null="true" column="`edamount`"/>
		<property name="edprice" not-null="true" column="`edprice`"/>
		<property name="edtotalprice" not-null="true" column="`edtotalprice`"/>
	</class>
</hibernate-mapping>